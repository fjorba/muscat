<%
  related_work = tag.fetch_first_by_tag("a").content rescue ""
	master_tag = @item.marc.config.get_master(tag.tag) rescue master_tag = ""
	db_id = tag.fetch_first_by_tag(master_tag).content rescue db_id = ""
-%>
<tr class="row">
	<th valign="top">
		<% if (no_label == false) -%>
			<%= @editor_profile.get_label(tag.tag) %>
		<%- end %>
	</th>
	<td valign="top">
    <%= link_to "#{related_work}", admin_work_path(db_id) rescue "" -%>
	</td>
</tr>
